{
  "python.formatting.blackArgs": [
    "--line-length 120"
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "commands": [
        "fuzzySearch.activeTextEditor"
      ],
      "before": [
        "<leader>",
        "<leader>"
      ]
    },
    {
      "before": [
        "<leader>",
        "i"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.terminal.focus"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "t"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.terminal.toggleTerminal"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "u"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.focusActiveEditorGroup"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "v"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.splitEditor"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "V"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.splitEditorDown"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "h"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.focusLeftGroup"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "l"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.focusRightGroup"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "p"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.showCommands"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "o"
      ],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "before": [
        "<leader>",
        "/"
      ],
      "after": [],
      "commands": [
        {
          "command": ":nohl"
        }
      ]
    },
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    },
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    }
  ],
  "debug.showBreakpointsInOverviewRuler": true,
  "shellcheck.customArgs": [
    "-x"
  ],
  "yaml.format.enable": false,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "python.languageServer": "Default",
  "terminal.integrated.confirmOnExit": "always",
  "[terraform]": {},
  "editor.renderWhitespace": "boundary",
  "files.insertFinalNewline": true,
  "terminal.integrated.showExitAlert": false,
  "window.title": "${folderName} :: ${activeEditorMedium} ",
  "emmet.includeLanguages": {
    "vue-html": "html"
  },
  "http.proxyStrictSSL": true,
  "emmet.triggerExpansionOnTab": true,
  "editor.guides.indentation": false,
  "editor.fontLigatures": "'ss02', 'ss19'",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "files.exclude": {
    "**/.idea": true,
    "**/*.pyc": true,
    "**/.DS_Store": true,
    "**/.terragrunt-cache": true,
    "**/__pycache__": true,
    "**/.svn": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true,
    "**/.vagrant": true,
    "**/.git": true,
    "**/.vscode": true
  },
  "python.linting.pycodestyleArgs": [
    "--max-line-length=160"
  ],
  "editor.autoClosingQuotes": "beforeWhitespace",
  "gitlens.hovers.currentLine.over": "line",
  // "vim.statusBarColors.visual": "#BF616A",
  // "vim.statusBarColors.normal": "#434C5E",
  // "vim.statusBarColors.visualline": "#BF616A",
  // "vim.statusBarColors.insert": "#EBCB8B",
  // "vim.statusBarColors.visualblock": "#BF616A",
  "vim.leader": "<space>",
  "vim.highlightedyank.duration": 300,
  "vim.highlightedyank.enable": true,
  "vim.useSystemClipboard": true,
  "vim.incsearch": true,
  "vim.easymotion": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.neovimUseConfigFile": true,
  "vim.hlsearch": true,
  // "vim.searchHighlightColor": "rgba(150, 255, 255, 0.3)",
  // "workbench.colorCustomizations": {
  //   "statusBar.background": "#434C5E",
  //   "statusBar.noFolderBackground": "#434C5E",
  //   "statusBar.debuggingBackground": "#434C5E"
  // },
  "python.formatting.autopep8Args": [
    "--max-line-length=160",
    "--ignore",
    "E402"
  ],
  "gitlens.statusBar.format": "${author}, ${date} (${ago}) - ${message}",
  "gitlens.hovers.enabled": false,
  "terminal.integrated.rightClickBehavior": "paste",
  "shellcheck.enableQuickFix": true,
  "files.trimFinalNewlines": true,
  "explorer.openEditors.visible": 0,
  "prettier.printWidth": 120,
  "gitlens.keymap": "alternate",
  "terminal.integrated.copyOnSelection": true,
  "yaml.format.printWidth": 160,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "files.trimTrailingWhitespace": true,
  "python.linting.pycodestyleEnabled": true,
  "editor.formatOnPaste": false,
  "editor.fontFamily": "'JetBrains Mono', 'Fira Code', 'SauceCodePro Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.suggestSelection": "first",
  "editor.lineNumbers": "relative",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.formatOnType": false,
  "editor.linkedEditing": true,
  "editor.formatOnSave": true,
  "editor.quickSuggestions": {
    "other": true,
    "strings": true,
    "comments": false
  },
  "editor.suggest.preview": true,
  "editor.minimap.enabled": false,
  "workbench.activityBar.visible": false,
  "workbench.iconTheme": "Monokai Pro (Filter Octagon) Icons",
  "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
  "terminal.external.osxExec": "iTerm.app",
  "git.untrackedChanges": "separate",
  "gitlens.defaultDateStyle": "absolute",
  "files.associations": {
    "Runfile": "makefile",
    "*.json.j2": "json",
    "*.sh.j2": "shellscript",
    "*.yaml.j2": "yaml",
    ".envrc": "shellscript",
    ".envrc.template": "shellscript"
  },
  "yaml.format.bracketSpacing": false,
  "go.useLanguageServer": true,
  "go.vetOnSave": "package",
  "explorer.confirmDragAndDrop": false,
  "go.toolsManagement.autoUpdate": true,
  "security.workspace.trust.untrustedFiles": "open",
  "redhat.telemetry.enabled": false,
  "gitlens.codeLens.enabled": false,
  "python.analysis.completeFunctionParens": true,
  "terraform.codelens.referenceCount": true,
  "go.formatTool": "goimports",
  "go.liveErrors": {
    "enabled": true,
    "delay": 500
  },
  "go.lintTool": "golangci-lint",
  "telemetry.telemetryLevel": "off",
  "go.inlayHints.constantValues": true,
  "go.inlayHints.functionTypeParameters": true,
  "terraform.languageServer.enable": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "shellformat.flag": "-i 2 -ci -bn",
  "shellformat.useEditorConfig": false,
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
}
